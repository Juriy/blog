<article class="post">
  <header>
  	<a href="<%= @post.url %>" alt="<%= @post.title %>"><%= @post.title %></a>
  </header>
  <span class="post-meta">
    <time><%= @formatDate(@post.date, 'MMMM DD, YYYY') %></time>
  </span>
  <section>
    <%- @post.contentRenderedWithoutLayouts %>
  </section>
</article>